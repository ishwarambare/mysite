{% include "head.html.j2" %}
<body>

    <div id="content">
        {% block content %}

        {% endblock %}
    </div>

    <div id="sidebar">
        <h2>All My Post</h2>
        <p>This is my blog. I've written {{total_posts()}} posts so far.</p>

         <h6>Latest post</h6>
        <ul>
        {% for post in show_latest_posts() %}
        <li>
        <a href="{{ post.get_absolute_url }}">{{ post.title }}</a>
        </li>
        {% endfor %}
        </ul>

    </div>

</body>
