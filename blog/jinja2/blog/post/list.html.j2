{% extends "base.html.j2" %}
{% block title %}My Blog{% endblock %}
{% block content %}
  <h1>My Blog</h1>
  {% for post in posts %}
    <h2><a href="{{ url ('blog:post_detail',kwargs={'id':post.id}) }}">{{ post.title }}</a></h2>
    <p class="date">
      Published {{ post.publish }} by {{ post.author }}
    </p>
  {% endfor %}

{% endblock %}

<meta property="og:url" content="{{ url ('hostel_view',kwargs={'hostel_slug':hostel.slug})}}">

<link rel="canonical" href="{{ url ('hostel_view',kwargs={'hostel_slug':hostel.slug})}}">
