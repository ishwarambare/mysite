{% extends "base.html.j2" %}
    {% block content %}
  <h1>My Blog</h1>
  {% for post in post %}
    <h2>
      <a href="{{ post.get_absolute_url }}">
        {{ post.title }}
      </a>
    </h2>
    <p class="date">
      Published {{ post.publish }} by {{ post.author }}
    </p>
  {% endfor %}
    {% endblock %}